# --- Azure Document Intelligence (Form Recognizer) ---
DOC_INTEL_ENDPOINT=your-doc-intel-endpoint-here
KEY=your-document-intelligence-key-here

# --- Azure Cosmos DB (MongoDB vCore) ---
COSMOS_MONGO_USER=your-cosmos-username
COSMOS_MONGO_PWD=your-cosmos-password
COSMOS_MONGO_SERVER=your-cosmos-server.mongo.cosmos.azure.com
MONGO_CLUSTER_NAME=your-cluster-name
COSMOS_DB_NAME=testdb
COSMOS_COLLECTION_NAME=col1

# --- OpenAI / Foundry API ---
# If using Azure OpenAI:
OPENAI_API_TYPE=azure
OPENAI_API_KEY=your-openai-api-key
OPENAI_API_VERSION=2025-12-11
AZURE_OPENAI_EMBEDDING_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-ada-002
AZURE_OPENAI_EMBEDDING_API_VERSION=2024-02-01
AZURE_FOUNDRY_CHAT_ENDPOINT=https://your-foundry-endpoint.cognitiveservices.azure.com
AZURE_FOUNDRY_API_KEY=your-foundry-api-key
AZURE_FOUNDRY_CHAT_DEPLOYMENT=gpt-5.2-chat

# --- Storage (Optional for raw file backup) ---
STORAGE_ACCOUNT_NAME=your-storage-account-name
STORAGE_ACCOUNT=https://your-storage-account.blob.core.windows.net/
STORAGE_KEY=your-storage-key
STORAGE_CON_NAME_RAW=your-storage-container-name

# --- Server Configuration ---
HOST=0.0.0.0
PORT=8000
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# --- Azure OpenAI API Configuration ---
AZURE_OPENAI_API_VERSION=2025-04-01-preview
